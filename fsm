digraph "State Machine" {
	graph [color=black directed=true fillcolor=white label="State Machine" rankdir=LR "strict"=false style=solid]
	node [color=black fillcolor=white peripheries=1 shape=rectangle style="rounded, filled"]
	edge [color=black]
	idle [label="idle\l" color=red fillcolor=darksalmon peripheries=2]
	search_filter [label="search_filter\l"]
	get_location [label="get_location\l"]
	get_radius [label="get_radius\l"]
	get_price [label="get_price\l"]
	get_keyword [label="get_keyword\l"]
	information [label="information\l"]
	idle -> information [label="advance [is_going_to_information]"]
	idle -> search_filter [label="advance [is_going_to_search_filter]"]
	search_filter -> information [label="advance [is_going_to_information]"]
	search_filter -> get_location [label="advance [is_going_to_get_location]"]
	search_filter -> get_radius [label="advance [is_going_to_get_radius]"]
	search_filter -> get_price [label="advance [is_going_to_get_price]"]
	search_filter -> get_keyword [label="advance [is_going_to_get_keyword]"]
	search_filter -> search_filter [label="advance [is_going_to_search_filter]"]
	search_filter -> idle [label="advance [is_going_to_idle]"]
	get_location -> information [label="advance [is_going_to_information]"]
	get_location -> search_filter [label="advance [is_going_to_search_filter]"]
	get_radius -> information [label="advance [is_going_to_information]"]
	get_radius -> search_filter [label="advance [is_going_to_search_filter]"]
	get_price -> information [label="advance [is_going_to_information]"]
	get_price -> search_filter [label="advance [is_going_to_search_filter]"]
	get_keyword -> information [label="advance [is_going_to_information]"]
	get_keyword -> search_filter [label="advance [is_going_to_search_filter]"]
	information -> idle [label=go_back]
	information -> select_type [label=go_back]
	information -> search_filter [label=go_back]
	information -> get_location [label=go_back]
	information -> get_radius [label=go_back]
	information -> get_price [label=go_back]
	information -> get_keyword [label=go_back]
}
